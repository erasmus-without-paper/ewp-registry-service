<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Package name="~https.github_com.*" />
    </Match>
    <Match>
        <Package name="~org.w3.*" />
    </Match>
    <Match>
        <!-- TODO those bugs should be brought back! It were added for the sake of update Spotbugs -->
        <!-- TODO Spotbugs ignores that fields are using annotations like @ValidatorTestStep (URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD) -->
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,MS_EXPOSE_REP,URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD"/>
    </Match>
</FindBugsFilter>
